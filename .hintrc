{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/time-and-media": [
      "default",
      {
        "meta-refresh": "off"
      }
    ],
    "no-inline-styles": "off",
    "disown-opener": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "text-align-last",
          "text-emphasis"
        ]
      }
    ],
    "axe/forms": "off",
    "axe/structure": [
      "default",
      {
        "listitem": "off"
      }
    ],
    "axe/name-role-value": [
      "default",
      {
        "button-name": "off",
        "link-name": "off"
      }
    ]
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_ff <= 110",
    "not firefox <= 111"
  ]
}